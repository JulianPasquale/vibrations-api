{
  "name": "vibrations-api",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "prestart": "yarn run build",
    "start": "node .",
    "debug": "DEBUG=express:* node . --inspect",
    "build": "yarn clean && tsc",
    "watch": "nodemon --watch src -e ts,json --exec yarn run start",
    "lint": "eslint . --ext .ts,.json",
    "lint:fix": "yarn lint --fix"
  },
  "engines": {
    "node": "~14.15.0",
    "yarn": "~1.22.0"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "firebase-admin": "^9.4.2",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.10",
    "nodemon": "^2.0.6",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.3",
    "typescript": "^4.1.2"
  }
}